#include<stdio.h>
typedef struct pnode
{
    float coef;
    int exp;
    struct pnode *next;
    
}p;
p *getnode();

void main()
{
    p *p1, *p2,*p3;
    //p *getpoly(), *add(p*,p*);
    //void display(p*);
    printf("/n enter first polynomial");
    p1=getpoly();
    printf("\n enter second polynomial");
    p2=getpoly();
    printf("\n the first polynomial is");
    display(p1);
    printf("\n the second polynomial is");
    display(p2);
    p3=add(p1,p2);
    printf("\n addition of two polynomial is :\n");
    display(p3);
}
